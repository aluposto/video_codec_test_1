# Run this to capture the exact import error and show top of the file for context
import traceback, sys, os
from pathlib import Path

print("CWD:", Path.cwd())
print("Exists:", Path("src/models/video_model.py").exists())
print("sys.path[:6]:", sys.path[:6])
print("-" * 60)
try:
    from src.models.video_model import DMC
    print("SUCCESS: imported DMC ->", DMC)
    import inspect
    try:
        print("DMC.__init__ signature:", inspect.signature(DMC.__init__))
    except Exception as e:
        print("Could not get signature:", e)
except Exception:
    print("IMPORT FAILED â€” full traceback below:")
    traceback.print_exc()
    print("\n--- Showing src/models/video_model.py (lines 1-140) for context ---\n")
    os.system("sed -n '1,140p' src/models/video_model.py")
