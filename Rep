# Run this in a notebook cell to capture the real import error
import traceback, sys
from pathlib import Path

print("CWD:", Path.cwd())
print("Checking file exists:", Path("src/models/video_model.py").exists())
print("First entries of sys.path:", sys.path[:8])

try:
    from src.models.video_model import DMC
    print("SUCCESS: imported DMC class:", DMC)
    import inspect
    sig = None
    try:
        sig = inspect.signature(DMC.__init__)
    except Exception:
        sig = "no signature available"
    print("DMC.__init__ signature:", sig)
except Exception as e:
    print("IMPORT FAILED:")
    traceback.print_exc()
